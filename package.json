    "cypress:component": "ELECTRON_ENABLE_LOGGING=1 cypress run --component",
    "cypress:e2e": "ELECTRON_ENABLE_LOGGING=1 start-server-and-test dev http://localhost:5173 'cypress run --e2e'",
    "test:e2e": "start-server-and-test dev http://localhost:5173 'cypress run'",
    "test:component": "cypress run --component",
    "pretest": "npm run build",
    "test": "npm run test:component && npm run test:e2e"
